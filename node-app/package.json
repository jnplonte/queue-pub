{
  "name": "queque-publisher",
  "description": "queque publisher",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "keywords": [],
  "author": "john paul onte <jnpl.onte@gmail.com>",
  "license": "MIT",
  "quokka": {
    "babel": true
  },
  "scripts": {
    "build:production": "concurrently \"tsc --project ./src/tsconfig.app.json\" \"npm run generate:config\"",
    "build:development": "concurrently \"tsc --project ./tsconfig.json\" \"npm run generate:config\" \"npm run generate:docs\"",
    "generate:config": "copyfiles -u 1 ./src/config/*.json ./dist",
    "generate:docs": "apidoc -i ./src/ -o ./dist/doc/ -f .ts",
    "start": "concurrently \"tsc --watch\" \"nodemon ./dist/index.js\"",
    "start:server": "node ./dist/index.js",
    "test": "concurrently \"tsc\" \"PORT=8881 TEST_DIR='./dist/doc' ARTIFACTS_DIR='./dist/doc' COVERAGE_DIR='./dist/doc' mocha dist/**/*.spec.js --timeout 100000 --exit\"",
    "test:one": "concurrently \"tsc\" \"mocha ./dist/app/v1/core/**/*.spec.js --timeout 100000 --no-coverage --exit\"",
    "lint": "tslint -c ./tslint.json './src/**/*.ts' -e './src/**/*.spec.ts'"
  },
  "dependencies": {
    "amqplib": "^0.5.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.3.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "fsevents": "^2.1.3",
    "helmet": "^3.23.3",
    "jnpl-helper": "^1.5.19",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.6",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.8",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "apidoc": "^0.25.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "tslint": "^6.1.3"
  }
}
